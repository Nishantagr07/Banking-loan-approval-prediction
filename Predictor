    "## Trainer function(Task 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "def train_model(X_train,y_train,data):\n",
    "    \n",
    "  #Data Preprocessing\n",
    "  from sklearn.preprocessing import StandardScaler\n",
    "  sc = StandardScaler()\n",
    "  X_train = sc.fit_transform(X_train)\n",
    "  #X_test = sc.transform(X_test)\n",
    "  return logistic_regression(X_train,y_train,data)\n",
    "  \n",
    "  \n",
    "  #logistic regression\n",
    "def logistic_regression(X_train,y_train,data):\n",
    "  from sklearn.linear_model import LogisticRegression\n",
    "  classifier = LogisticRegression(random_state = 0)\n",
    "  reslg=classifier.fit(X_train, y_train)\n",
    "  return reslg\n",
    "\n",
    "X=cleaned_df.iloc[:,:-1].values\n",
    "y=cleaned_df.iloc[:,-1].values   \n",
    "    \n",
    "from sklearn.model_selection import train_test_split\n",
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = 0.25, random_state = 0) \n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "LogisticRegression(C=1.0, class_weight=None, dual=False, fit_intercept=True,\n",
       "                   intercept_scaling=1, l1_ratio=None, max_iter=100,\n",
       "                   multi_class='warn', n_jobs=None, penalty='l2',\n",
       "                   random_state=0, solver='warn', tol=0.0001, verbose=0,\n",
       "                   warm_start=False)"
      ]
     },
     "execution_count": 26,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "train_model(X_train,y_train,cleaned_df)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Since we have maximum accuracy for logistic regression model, we construct the trainer and tester function on that model"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Tester function(Task 3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "def test_function(X_test,y_test,data):\n",
    "    \n",
    "    from sklearn.metrics import confusion_matrix, accuracy_score\n",
    "    y_pred = logistic_regression(X_test,y_test,data).predict(X_test)\n",
    "    cm = confusion_matrix(y_test, y_pred)\n",
    "   \n",
    "    return cm,accuracy_score(y_test, y_pred)"
   ]
  },
  {
